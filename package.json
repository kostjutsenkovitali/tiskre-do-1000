{
  "name": "tiskre-do-300",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "prebuild": "node scripts/disable-dynamic-for-export.js",
    "build": "next build",
    "sitemap": "next-sitemap",
    "preexport": "npm run sitemap",
    "start": "next start",
    "lint": "next lint",
    "clean": "rimraf .next out out.zip",
    "export": "next export",
    "postexport": "node scripts/make-htaccess.js && node scripts/restore-dynamic-after-export.js",
    "zip:out": "powershell -NoProfile -ExecutionPolicy Bypass -Command \"Compress-Archive -Path out\\* -DestinationPath out.zip -Force\"",
    "prepare:out": "npm run clean && npm run build && npm run export && npm run zip:out"
  },
  "dependencies": {
    "@shopify/storefront-api-client": "^1.0.9",
    "cannon-es": "^0.20.0",
    "graphql": "^16.11.0",
    "graphql-request": "^7.2.0",
    "lucide-react": "^0.464.0",
    "next": "15.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sanitize-html": "^2.17.0",
    "three": "^0.179.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/three": "^0.179.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
